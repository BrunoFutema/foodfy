<div class="items">
  <div class="item">
    <strong>Nome</strong>
    <div>
      <input
        type="text"
        name="name"
        placeholder="Digite o nome do chef"
        value="{{ chef.name }}"
      >
    </div>
  </div>

  <div class="item">
    <strong>Avatar</strong>
    <div>
      <input
        type="url"
        name="avatar_url"
        placeholder="https://"
        value="{{ chef.avatar_url }}"
      >
    </div>
  </div>
</div>

<div class="items">
  <div class="item">
    <strong>Nome</strong>
    <div>
      <input
        type="text"
        name="name"
        placeholder="Digite o nome completo"
        value="{{ user.name }}"
      >
    </div>
  </div>
  <div class="item">
    <strong>Email</strong>
    <div>
      <input
        type="email"
        name="email"
        placeholder="Digite o email"
        value="{{ user.email }}"
      >
    </div>
  </div>
</div>
